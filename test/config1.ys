import: YAML::PP
ypp =: YAML::PP->new()

yaml-file =: "$(dirname *file*)/config1-defaults.yaml"

defaults =:
  ypp->load_file(yaml-file)

entries =: !
  - size: 43

  - color: pink
    fast: true
    cool: [1, 2, 3]

  - {}

  - fast: true
    other: stuff

data =:
  vec:
    map:
    - fn (m): merge(defaults m)
    - entries

say: ypp->dump_string(data)
